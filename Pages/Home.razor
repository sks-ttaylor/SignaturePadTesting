@page "/"

<PageTitle>Home</PageTitle>
<Row>
  <Column>
    <Card>
      <CardHeader>
        <CardTitle>Signature pad</CardTitle>
      </CardHeader>
      <CardBody>
        <SignaturePad CanvasHeight="150"
                      CanvasWidth="300"
                      ImageType="SignaturePadImageType.Jpeg"
                      ImageQuality="1"
                      Border="Border.Is1.Secondary"
                      Value=@bytes />
      </CardBody>
    </Card>
  </Column>
</Row>
<Row>
  <Column>
    <Card>
      <CardHeader>
        <CardTitle>Preview</CardTitle>
      </CardHeader>
      <CardBody>
        <Image Source="@DataUrl" 
          Border="Border.Is1.Secondary"
          Height="Height.Px(CanvasHeight)" 
          Width="Height.Px(CanvasWidth)" />
      </CardBody>
    </Card>
  </Column>
</Row>

@code {

  int CanvasHeight = 150;
  int CanvasWidth = 300;

  string DataUrl => "data:image/jpeg;base64,";

  byte[] bytes => Convert.FromBase64String(DataUrl![(DataUrl!.IndexOf(',') + 1)..]);
}

